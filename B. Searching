#include<stdio.h>
int a[10000000];
int main()
{
    int n, index=-1;

    scanf("%d",&n);

    for(int i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }

    int x;
    scanf("%d",&x);
    for(int i=0; i<n; i++)
    {
        if(a[i] == x)
        {
            index=i;
            break;
        }
    }
    printf("%d\n",index);
}
