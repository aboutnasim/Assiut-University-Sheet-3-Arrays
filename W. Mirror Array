#include<stdio.h>

int main()
{
    int i,k,j,n,m;

    int a[100][100];

    scanf("%d %d",&n, &m);

    for(i=0; i<n; i++)
    {
        for(j=0; j<m; j++)
        {
            scanf("%d", &a[i][j]);
        }
    }


    for(i=0; i<n; i++)
    {
        for(j=m-1; j>=0; j--)
        {
            printf("%d ",a[i][j]);
        }
        printf("\n");
    }
}
