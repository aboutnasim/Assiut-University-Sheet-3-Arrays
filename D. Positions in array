#include<stdio.h>
int a[1000000];
int main()
{
    int n;
    scanf("%d",&n);

    for(int i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
    

    for(int i=0; i<n; i++)
    {
        if(a[i]<=10)
        {
            printf("A[%d] = %d\n",i, a[i]);
        }
        else
        {
            continue;
        }
    }
}
